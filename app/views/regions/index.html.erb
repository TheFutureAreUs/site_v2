<div class="college_header">
  <div class="top_box">
    <h1 id="title_list">List of Colleges</h1>
    <p id="title_desc">
      Below is a list of colleges and universities
    </p>
  </div>
</div>
<div class="container-fluid main-container">
  <div class="">

    <div class="">
        <div><h2 class="header_college">Maryland</h2></div>
      <div class="college_wrapper">
          <% @maryland_colleges.colleges.each do |college| %>
              <%= link_to college.name, region_college_path(@maryland_colleges, college), class: 'college_names' %>
          <% end %>
      </div>
      <div><h2>Georgia</h2></div>
    </div>
  </div>
</div>