<div class="review-rating" data-score="<%= review.rating %>"></div>
<p class="review-comment"><%= review.comment %></p>

<% if user_signed_in? %>

  <% if review.user_id == current_user.id %>

    <%= link_to "Edit", edit_listing_review_path(review.listing, review) %>
    <%= link_to "Delete", listing_review_path(review.listing, review), method: :delete, data: {confirm: "Are you sure?"} %>
  
  <% end %>

<% end %>